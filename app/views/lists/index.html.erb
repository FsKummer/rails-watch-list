<h1>These are all your lists</h1>

<% @lists.each do |list| %>
  <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key if list.photo.attached? %>')">
    <h2>
      <%= link_to list.name, list_path(list)%>
    </h2>
  </div>
<% end %>
